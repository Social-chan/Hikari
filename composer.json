{
  "name": "socialchan/hikari",
  "description": "Repositories with cache for Laravel 5+",
  "keywords": [
    "laravel",
    "repository",
    "repository-pattern",
    "package",
    "cache-busting"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "D8vjork",
      "email": "d8vjork@outlook.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "~5.1",
    "illuminate/database": "~5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Socialchan\\Hikari\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/TestFunctions.php"
    ],
    "psr-4": {
      "Socialchan\\Hikari\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "archive" : {
    "exclude": [
      "/tests"
    ]
  }
}
